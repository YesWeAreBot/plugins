  "repository": {
    "type": "git",
    "url": "git+https://github.com/YesWeAreBot/YesImBot.git",
    "directory": "packages/code-executor"
  },
  "bugs": {
    "url": "https://github.com/YesWeAreBot/YesImBot/issues"
  },
  "dependencies": {
    "pyodide": "^0.29.0"
  },
  "devDependencies": {
    "koishi": "^4.18.7",
    "koishi-plugin-yesimbot": "^3.1.0-beta.1"
  },
